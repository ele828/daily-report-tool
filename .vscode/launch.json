  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/index.ts",
      "outFiles": ["${workspaceRoot}/dist/index.js"],
      "sourceMaps": true,
      "stopOnEntry": false,
      "args": [],
